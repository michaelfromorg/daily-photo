{
	"expo": {
		"name": "daily-notion",
		"slug": "daily-notion",
		"scheme": "dailynotion",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"package": "com.michaelfromyeg.dailynotion"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-secure-store",
			[
				"expo-notifications",
				{
					"icon": "./assets/notification-icon.png"
				}
			],
			[
				"expo-font",
				{
					"fonts": [
						"node_modules/@expo-google-fonts/inter/900Black/Inter_900Black.ttf"
					]
				}
			],
			"expo-web-browser"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "3896899d-b7ff-46a7-9d7a-276153ef1a92"
			}
		}
	}
}
